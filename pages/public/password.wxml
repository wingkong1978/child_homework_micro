<!--index.wxml-->
<view style="text-align:center"> {{classname}} 暗号</view>
<view class="page">
  <view class="page__hd">
  </view>
  <view class="page__bd">
    
    <form bindsubmit="formSubmit" bindreset="formReset">
        <view class="weui-cells__title">暗号：</view>
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输入暗号" name="password" password maxlength="8" type='number'/>
                </view>
            </view>
        </view>
      <view class="weui-cell">
        <button class="weui-btn confirmBtn" formType="submit">确定</button>
        <button formType="reset" type='default'>重设</button>
      </view>



    </form>
  </view>
</view>